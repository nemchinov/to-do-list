{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "modules": true,
            "experimentalObjectRestSpread": true
        }
    },
    "plugins": [
        "react"
    ],
    "extends": [
        "airbnb-base",
        "plugin:react/recommended",
        "eslint:recommended"
    ],
    "rules": {
        "semi": 2
    },
    "settings": {
        "react": {
            "pragma": "React"
        }
    }
}